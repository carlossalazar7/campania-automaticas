        <class_source><![CDATA[
import org.json.simple.JSONArray;
import org.json.simple.JSONObject;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
		Object[] r = getRow();
		if (r == null) {
			setOutputDone();
			return false;
		}

    	if (first)
	    {
        	first = false;
  		}

		// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    	// enough to handle any new fields you are creating in this step.
    	Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
		//Object[] outputRow = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
		   
		//Get the value from an input field
		String CUSTOMER_NAME = get(Fields.In, "NOMBRE_CLIENTE").getString(r);
System.out.println( CUSTOMER_NAME);		
        String CUSTOMER_EMAIL = get(Fields.In, "CORREOELECTRONICO").getString(r);
		String NAME_SENDER = get(Fields.In, "NAME_SENDER").getString(r);
		String DIR_SENDER = get(Fields.In, "DIR_SENDER").getString(r);
		String ASUNTO = get(Fields.In, "ASUNTO").getString(r);
		String PLANTILLA_HTML = get(Fields.In, "PLANTILLA_HTML").getString(r);
		String CICLO = "CAMPAUTOMATICA";//get(Fields.In, "CICLO").getString(r);
		String CORREL_NOTIF = get(Fields.In, "CORREL_NOTIF").getString(r);
		String MANDRILL_API_KEY = get(Fields.In, "MANDRILL_API_KEY").getString(r);
		
        java.text.SimpleDateFormat sdf = new java.text.SimpleDateFormat("dd/MM/yyyy");
        String fechaActual = sdf.format(new java.util.Date());


		/** Estructura del request json a la api mandrill
		{
			"key": "Mandrill api key",
			"message": {
				"html": "<p>Example HTML content</p>",
				"text": "Example text content",
				"subject": "example subject",
				"from_email": "RecordatorioPagoSV@simaninternet.com",
				"from_name": "Recordatorio Credisiman",
				"to": [{
					"email": "josue_salazar@siman.com",
					"name": "JOSUE RUIZ SALAZAR",
					"type": "to"
				}],
				"important": false,
				"tags": ["Premora Ciclo 21 Notificación de Pago 1 24/Nov/2016 "]
			}
		}
		*/

		JSONObject json = new JSONObject();
		//json.put("key", getVariable("V_SMTP_PASS"));

		//json.put("key", "kwdXSolNtPMoFsBXKSgZDQ");
		json.put("key", MANDRILL_API_KEY);
		JSONObject msg = new JSONObject();
		json.put("message", msg);
		msg.put("html", PLANTILLA_HTML);
		msg.put("subject", ASUNTO);
		msg.put("from_email", DIR_SENDER);
		msg.put("from_name", NAME_SENDER);
		msg.put("important", "false");

		JSONArray to = new JSONArray();
		JSONObject dest = new JSONObject();
		dest.put("email", CUSTOMER_EMAIL);
		dest.put("name", CUSTOMER_NAME);
		dest.put("type", "to");
		to.add(dest);
		msg.put("to", to);
		
		JSONArray tags = new JSONArray();
		tags.add("NotificacionNoEntregada"+CICLO);
		msg.put("tags", tags);
		 
		
		//Set a value in a new output field
    	get(Fields.Out, "F_JSON_REQUEST").setValue(outputRow, json.toString());


		// Send the row on to the next step.
    	putRow(data.outputRowMeta, outputRow);

		return true;
}
